# Application Configuration
APP_NAME=devconf_multi_agent
API_ROUTER_PATH_PREFIX=/api/devconf/v1

# OpenAI Compatible Host
OPENAI_COMPATIBLE_HOST=http://localhost:11434    # ollama's openai compatible host
OPENAI_API_KEY=your_openai_api_key_here

# Agent Models
SUPERVISOR_MODEL=openai/gpt-oss-120b
GOOGLE_SEARCH_AGENT_MODEL=openai/gpt-oss-120b
GITHUB_SEARCH_AGENT_MODEL=openai/gpt-oss-120b

# A2A Agent Base URLs
GOOGLE_SEARCH_AGENT_BASE_URL=http://localhost:8001
GITHUB_AGENT_BASE_URL=http://localhost:8002

# SSL Verification
VERIFY_SSL=False
DEFAULT_TIMEOUT=30

# Granite Guardian (Input Guardrails)
INPUT_GUARDRAILS_ENABLED=True
GRANITE_GUARDIAN_HOST=your_granite_guardian_host
GRANITE_GUARDIAN_API_KEY=your_granite_guardian_key
GRANITE_SHIELD_ID=ibm-granite/granite-guardian-3.3-8b

# Langfuse Tracing
LANGFUSE_TRACING_ENABLED=False
LANGFUSE_SECRET_KEY=your_langfuse_secret_key
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_BASE_URL=http://localhost:3000

# Database Configuration
USE_DATABASE_SESSIONS=False
DATABASE_URL=mysql+pymysql://user:password@localhost:3306/devconf_db
